{
  "files.exclude": {
    "node_modules": true,
    "netlify.js": true,
    ".gitignore": true,
    "desktop.ini": true,
    "package-lock.json": true,
    "yarn.lock": true,
    "tsconfig.json": true,
    "static": true,
    "dist": true,
    "plugins": true,
    "package.json": true,
    "config": true,
    "sanity.json": true
  },

  "files.associations": {
    "*.js": "javascriptreact"
  },

  "vsicons.associations.folders": [
    {
      "icon": "config",
      "extensions": ["react"]
    },
    {
      "icon": "paket",
      "extensions": ["schemas"]
    }
  ],
  "cSpell.language": "nl, en"
}
